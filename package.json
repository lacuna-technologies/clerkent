{
  "name": "clerkent",
  "version": "1.0.9",
  "private": true,
  "description": "quickly locate, view, and download judgments",
  "repository": "https://github.com/lacuna-technologies/clerkent.git",
  "license": "MIT",
  "author": {
    "name": "Huey",
    "email": "hello@huey.xyz"
  },
  "scripts": {
    "build": "npm run build:chrome && npm run build:firefox && npm run build:opera",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack",
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch --progress",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch --progress",
    "dev:opera": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch --progress",
    "generate:icons": "babel-node scripts/generateIcons.ts --extensions \".ts\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:staged": "lint-staged",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,d.ts}": [
      "tsc --allowSyntheticDefaultImports --jsx react --noEmit --esModuleInterop --moduleResolution node --skipLibCheck --resolveJsonModule --downlevelIteration --lib ESNEXT,ES2020.String,dom"
    ],
    "*.{ts,tsx,js,jsx,json}": [
      "eslint --fix --ignore-pattern"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "advanced-css-reset": "^1.2.2",
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.3",
    "cheerio": "^1.0.0-rc.9",
    "memoizee": "^0.4.15",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "sharp": "^0.27.2",
    "webext-base-css": "^1.3.1",
    "webextension-polyfill-ts": "^0.22.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-destructuring": "^7.13.17",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/cheerio": "^0.22.28",
    "@types/jest": "^26.0.23",
    "@types/memoizee": "^0.4.5",
    "@types/node": "^14.14.44",
    "@types/qs": "^6.9.6",
    "@types/react": "^16.14.6",
    "@types/react-dom": "^16.9.12",
    "@types/sharp": "^0.27.3",
    "@types/webpack": "^4.41.28",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
