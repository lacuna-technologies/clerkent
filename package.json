{
  "name": "clerkent",
  "version": "2.9.0",
  "private": true,
  "description": "quick search for international caselaw and legislation",
  "repository": "https://github.com/lacuna-technologies/clerkent.git",
  "license": "GPL-3",
  "author": {
    "name": "Huey",
    "email": "hello@huey.xyz"
  },
  "scripts": {
    "build": "npm run build:chrome && npm run build:firefox && npm run build:opera",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack",
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch --progress",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch --progress",
    "dev:opera": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch --progress",
    "generate:icons": "babel-node scripts/generateIcons.ts --extensions \".ts\"",
    "lint": "eslint . --ext .ts,.tsx --cache",
    "lint:fix": "eslint . --ext .ts,.tsx --cache --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage && codecov",
    "test:clear-cache": "jest --clearCache",
    "version": "babel-node scripts/getVersion.ts --extensions \".ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,d.ts}": [
      "tsc-files --noEmit src/types/Filetypes.d.ts"
    ],
    "*.{ts,tsx,js,jsx,json}": [
      "eslint --cache --fix"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "axios": "^0.27.2",
    "axios-cache-adapter": "^2.7.3",
    "cheerio": "^1.0.0-rc.10",
    "fuse.js": "^6.6.0",
    "leven": "^4.0.0",
    "memoizee": "^0.4.15",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-destructuring": "^7.17.7",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^27.5.0",
    "@types/memoizee": "^0.4.7",
    "@types/node": "^17.0.31",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sharp": "^0.30.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.2",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^42.0.0",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-scss-transform": "^1.0.1",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^1.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.4.13",
    "postcss-loader": "^4.2.0",
    "react-test-renderer": "^17.0.2",
    "resolve-url-loader": "^3.1.3",
    "sass": "^1.51.0",
    "sass-loader": "^10.1.1",
    "sharp": "^0.30.4",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsc-files": "^1.1.3",
    "ttf-loader": "^1.0.2",
    "typescript": "^4.6.4",
    "webextension-polyfill-ts": "^0.25.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-extension-reloader": "^1.1.4",
    "wext-manifest-loader": "^2.3.0",
    "wext-manifest-webpack-plugin": "^1.2.1"
  },
  "os": [
    "linux"
  ]
}
