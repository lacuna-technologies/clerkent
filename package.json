    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.3",
    "sass-loader": "^10.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
