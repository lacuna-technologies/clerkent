{
  "name": "clerkent",
  "version": "3.2.1",
  "private": true,
  "description": "quick search legal search",
  "repository": "https://github.com/lacuna-technologies/clerkent.git",
  "license": "GPL-3",
  "author": {
    "name": "Huey",
    "email": "hello@huey.xyz"
  },
  "scripts": {
    "build": "npm run build:chrome && npm run build:firefox && npm run build:opera",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack",
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch --progress",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch --progress",
    "dev:opera": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch --progress",
    "generate:icons": "babel-node scripts/generateIcons.ts --extensions \".ts\"",
    "lint": "eslint . --ext .ts,.tsx --cache",
    "lint:fix": "eslint . --ext .ts,.tsx --cache --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage && codecov",
    "test:clear-cache": "jest --clearCache",
    "version": "babel-node scripts/getVersion.ts --extensions \".ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": [
      "eslint --cache --fix"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "fuse.js": "^6.6.2",
    "leven": "^4.0.0",
    "memoizee": "^0.4.15",
    "preact": "^10.10.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/preact": "^3.2.2",
    "@types/jest": "^28.1.6",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.0.6",
    "@types/qs": "^6.9.7",
    "@types/sharp": "^0.30.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "codecov": "^3.8.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^43.0.2",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "sharp": "^0.30.7",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^3.1.6",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.7.4",
    "webextension-polyfill-ts": "^0.26.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "wext-manifest-loader": "^2.4.1",
    "wext-manifest-webpack-plugin": "^1.2.1"
  },
  "os": [
    "linux"
  ]
}
